// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int arr[100],n;
    printf("Enter the number of elements:");
    scanf("%d",&n);
    for(int i=0;i<n;i++){
        printf("Enter the array element %d:",i);
        scanf("%d",&arr[i]);
    }
    int *p=arr;

    for(int i=n-1;i>=0;i--){
        printf("%d",*(p+i));
    }

    return 0;
}
