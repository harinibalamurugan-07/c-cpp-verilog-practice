// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n,count=0;
    printf("Enter:");
    scanf("%d",&n);
    for(int i=2;i<=n;i++){
        if(n%i==0){
            count++;
        }
    }
    printf("%d\n",count);
    if(count<=1){
        printf("prime");
    }else{
        printf("not prime");
    }
    return 0;
}
